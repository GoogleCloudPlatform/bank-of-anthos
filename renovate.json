{
  "extends": [
    "config:base"
  ],
  "pip-compile": {
    "enabled": true,
    "fileMatch": ["(^|/)requirements\\.in$"]
  },
  "constraints": {
    "python": "~=3.10.0"
  },
  "prConcurrentLimit":5,
  "stabilityDays":7,
  "vulnerabilityAlerts":{
     "labels":[
       "type:security" 
     ],
     "stabilityDays":0
  },
  "labels": ["dependencies"],
  "python":{
    "addLabels": ["lang: python"]
  },
  "java":{
    "addLabels": ["lang: java"]
  },
  "golang":{
    "addLabels": ["lang: go"]
  }
}
